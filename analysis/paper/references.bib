@article{hunterFoodFeedingEcology1983,
  title = {The Food and Feeding Ecology of the Giant Petrels {{{\emph{Macronectes}}}}{\emph{ Halli}} and {{{\emph{M}}}}{\emph{. Giganteus}} at {{South Georgia}}},
  author = {Hunter, S.},
